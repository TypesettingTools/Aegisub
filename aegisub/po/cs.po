msgid "Bias: Start <- "
msgstr "Jezdec: Počátek <-"

#: dialog_timing_processor.cpp:126
msgid " -> End"
msgstr "-> Konec"

#: dialog_timing_processor.cpp:129
msgid "Keyframe snapping"
